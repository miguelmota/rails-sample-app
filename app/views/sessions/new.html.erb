<% provide(:title, "Sign in") %>
<h1>Sign in</h1>

<%= form_for(:sessions, url: sessions_path) do |f| %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>

<% end %>

<p><%= link_to "Sign up now!", signup_path %>
